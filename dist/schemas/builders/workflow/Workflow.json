{
    "$id": "https://schemas.toolproof.com/v0/Workflow.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://schemas.toolproof.com/v0/Base.json#/$defs/Identifiable"
        },
        {
            "type": "object",
            "properties": {
                "id": {
                    "$ref": "https://schemas.toolproof.com/v0/IdLib.json#/$defs/WorkflowId"
                },
                "steps": {
                    "type": "array",
                    "items": {
                        "$ref": "https://schemas.toolproof.com/v0/Step.json"
                    }
                }
            },
            "required": [
                "steps"
            ]
        }
    ],
    "unevaluatedProperties": false
}
