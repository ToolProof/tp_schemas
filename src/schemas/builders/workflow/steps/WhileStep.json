{
    "$id": "https://schemas.toolproof.com/v0/WhileStep.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "allOf": [
        {
            "$ref": "https://schemas.toolproof.com/v0/Base.json#/$defs/Identifiable"
        },
        {
            "type": "object",
            "properties": {
                "id": {
                    "$ref": "https://schemas.toolproof.com/v0/IdLib.json#/$defs/WhileStepId"
                },
                "kind": {
                    "const": "while"
                },
                "case": {
                    "$ref": "https://schemas.toolproof.com/v0/ConditionalWrapper.json"
                }
            },
            "required": [
                "kind",
                "case"
            ]
        }
    ],
    "unevaluatedProperties": false
}